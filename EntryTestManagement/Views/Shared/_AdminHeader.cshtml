@{
    var @styleURL = "/Content/styles/img";
}
<div class="wrapper">
    <div data-active-color="white" data-background-color="black" data-image class="app-sidebar">
        <div class="sidebar-header">
            <div class="logo clearfix">
                <a href="@Url.Action("index","Admin")" class="logo-text float-left">
                    <div class="logo-img"><img src="@styleURL/gallery/logo.png" style="width: 120%;" /></div><span class="text align-middle">FCIT</span>
                </a><a id="sidebarToggle" href="javascript:;" class="nav-toggle d-none d-sm-none d-md-none d-lg-block">
                    <i data-toggle="expanded" class="toggle-icon ft-toggle-right"></i>
                </a><a id="sidebarClose"
                       href="javascript:;" class="nav-close d-block d-md-block d-lg-none d-xl-none"><i class="ft-x"></i></a>
            </div>
        </div>
        <div class="sidebar-content">
            <div class="nav-container">
                <ul id="main-menu-navigation" data-menu="menu-navigation" data-scroll-to-active="true"
                    class="navigation navigation-main">
                    <li class="nav-item">
                        <a href="/Admin/Index">
                            <i class="ft-home"></i><span data-i18n=""
                                                         class="menu-title">Home</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        @{
                            var id = @Session["AdminID"];
                        }
                        <a href="@Url.Action("AdminProfile","Admin",new {@id})">
                            <i class="ft-user"></i><span data-i18n=""
                                                         class="menu-title">My Profile</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("ViewAdmins","Admin")">
                            <i class="ft-grid"></i><span data-i18n=""
                                                         class="menu-title">View Admins</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("ViewUsers","Admin")">
                            <i class="ft-layout"></i><span data-i18n=""
                                                           class="menu-title">View Users</span>
                        </a>
                    </li>
                    <li class="has-sub nav-item">
                        <a href="#"><i class="ft-grid"></i><span data-i18n="" class="menu-title">Manage Venue</span></a>
                        <ul class="menu-content">
                            <li>
                                <a href="@Url.Action("Groups","Admin")" class="menu-item">Groups</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Halls","Admin")" class="menu-item">Halls</a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item">
                        <a href="@Url.Action("ViewComplaints","Admin")">
                            <i class="ft-mail"></i><span data-i18n=""
                                                         class="menu-title">View Complaints</span>
                        </a>
                    </li>
                </ul>
                <div class="recover-pass" style="margin-left:5.5rem;">
                    <a href="@Url.Action("Logout","Admin")" class="btn btn-primary text-decoration-none text-white">Logout</a>
                </div>
            </div>
        </div>
        <div class="sidebar-background" style="background-image: url('/Content/styles/img/sidebar-bg/05.jpg')"></div>
    </div>
</div>